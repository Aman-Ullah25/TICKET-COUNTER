let age = 65


if (age<5 && age >0){
    console.log("Free for under 5 !!")
}
else if (age>5 && age<12){
    console.log("your ticket rupees should be 10$")
}
else if (age>13 && age<19){
    console.log(" your ticket rupees should be 20$")
}
else if(age>19 && age<40){
    console.log("Your ticket rupees shpuld be 30$ ")
}
else if(age>40 && age<100){
    console.log("Free for above 40 !!")
}
else{console.log("Kindly enter your age!!!!!!")}
